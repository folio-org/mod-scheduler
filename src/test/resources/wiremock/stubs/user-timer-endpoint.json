{
  "request": {
    "method": "POST",
    "url": "/test",
    "headers": {
      "Content-Type": {
        "equalTo": "application/json"
      },
      "x-okapi-module-hint": {
        "equalTo": "mod-foo"
      },
      "x-okapi-tenant": {
        "equalTo": "test"
      },
      "x-okapi-request-id": {
        "matches" : "^\\d{6}$"
      },
      "x-okapi-url": {
        "matches" : "^http://[a-zA-Z0-9.-]+:\\d{1,5}$"
      }
    }
  },
  "response": {
    "status": 201,
    "headers": {
      "Content-Type": "application/json"
    },
    "jsonBody": {
      "message": "success!"
    }
  }
}
